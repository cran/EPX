Package: EPX
Type: Package
Title: Ensemble of Phalanxes
Version: 1.0.3
Authors@R: c(
    person("Jabed", "Tomal", email = "jtomal@tru.ca", role = c("aut", "cre")),
    person("Grace", "Hsu", email = "grace.hsu.ubc@gmail.com", role = c("aut")),
